<!DOCTYPE html>
<html lang="en">
<head>
    <% include partials/head.ejs %>
</head>
<body id="body" data-spy="scroll" data-target=".one-page-header" class="demo-lightbox-gallery">
<main class="wrapper" ng-app="iMovieUi">
    <header>
        <% include partials/header.ejs %>
    </header>


    <div class="container" style="margin-top: 100px" ng-controller="ViewCtrl">
<div class="row">
    <div class="col-md-3">
        <div class="text-center" style="text-align: center;
    padding: 2px 45px;
    border: 2px solid grey;">
            <img ng-src="{{character.imgUrl}}" class="img-responsive"/>
        </div>
        <div>
           <span style="float:left">
                10 <i class="fa fa-star"></i>
           </span>
            <div class="progress">
                <div class="progress-bar" role="progressbar" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100" style="width:0%">
                    60%
                </div>
            </div>
        </div>
        <div>
            <span style="float:left">
                9 <i class="fa fa-star"></i>
           </span>
            <div class="progress">
                <div class="progress-bar" role="progressbar" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100" style="width:21%">
                    21%
                </div>
            </div>
        </div>
        <div>
            <span style="float:left">
                8 <i class="fa fa-star"></i>
           </span>
            <div class="progress">
                <div class="progress-bar" role="progressbar" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100" style="width:12%">
                    12%
                </div>
            </div>
        </div>
        <div>
           <span style="float:left">
                7 <i class="fa fa-star"></i>
           </span>
            <div class="progress">
                <div class="progress-bar" role="progressbar" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100" style="width:7%">
                    7%
                </div>
            </div>
        </div>
        <div>6 <i class="fa fa-star"></i></div>
        <div>5 <i class="fa fa-star"></i></div>
        <div>4 <i class="fa fa-star"></i></div>
        <div> 3 <i class="fa fa-star"></i></div>
        <div>2 <i class="fa fa-star"></i></div>
        <div>1 <i class="fa fa-star"></i></div>
    </div>
</div>
    </div>

    <footer>
        <% include partials/footer.ejs %>
    </footer>

    <% include partials/scripts.ejs %>
</main>
</body>
</html>